<?xml version="1.0"?>
<TestCases system="Integer">
  <Has name="server" value="/"/>
  <Has name="version" value="0.0.0"/>
  <Function name="POST_integer">
    <Has name="server" value="/"/>
    <Has name="version" value="0.0.0"/>
    <TestCase id="0">
      <Has name="properties" value="param0,param0Value"/>
      <Has name="server" value="/"/>
      <Has name="version" value="0.0.0"/>
      <Input type="query">
        <Var name="param0.Defined" value="Yes">
          <Has name="style" value="form"/>
        </Var>
        <Var name="param0.Type" value="integer"/>
        <Var name="param0.Value" value="100"/>
      </Input>
    </TestCase>
    <TestCase id="1">
      <Has name="server" value="/"/>
      <Has name="version" value="0.0.0"/>
      <Input type="query">
        <Var name="param0.Defined" value="No">
          <Has name="style" value="form"/>
        </Var>
        <Var name="param0.Type" NA="true"/>
        <Var name="param0.Value" NA="true"/>
      </Input>
    </TestCase>
    <TestCase id="2">
      <Has name="properties" value="param0,param0Value"/>
      <Has name="server" value="/"/>
      <Has name="version" value="0.0.0"/>
      <Input type="query">
        <Var name="param0.Defined" value="Yes">
          <Has name="style" value="form"/>
        </Var>
        <Var name="param0.Type" value="integer"/>
        <Var name="param0.Value" value="&gt; 100"/>
      </Input>
    </TestCase>
    <TestCase id="3" failure="true">
      <Has name="properties" value="param0"/>
      <Has name="server" value="/"/>
      <Has name="version" value="0.0.0"/>
      <Input type="query">
        <Var name="param0.Defined" value="Yes">
          <Has name="style" value="form"/>
        </Var>
        <Var name="param0.Type" value="null" failure="true"/>
        <Var name="param0.Value" NA="true"/>
      </Input>
    </TestCase>
    <TestCase id="4" failure="true">
      <Has name="properties" value="param0"/>
      <Has name="server" value="/"/>
      <Has name="version" value="0.0.0"/>
      <Input type="query">
        <Var name="param0.Defined" value="Yes">
          <Has name="style" value="form"/>
        </Var>
        <Var name="param0.Type" value="Not integer" failure="true"/>
        <Var name="param0.Value" NA="true"/>
      </Input>
    </TestCase>
    <TestCase id="5" failure="true">
      <Has name="properties" value="param0,param0Value"/>
      <Has name="server" value="/"/>
      <Has name="version" value="0.0.0"/>
      <Input type="query">
        <Var name="param0.Defined" value="Yes">
          <Has name="style" value="form"/>
        </Var>
        <Var name="param0.Type" value="integer"/>
        <Var name="param0.Value" value="Not a multiple of 2" failure="true"/>
      </Input>
    </TestCase>
    <TestCase id="6" failure="true">
      <Has name="properties" value="param0,param0Value"/>
      <Has name="server" value="/"/>
      <Has name="version" value="0.0.0"/>
      <Input type="query">
        <Var name="param0.Defined" value="Yes">
          <Has name="style" value="form"/>
        </Var>
        <Var name="param0.Type" value="integer"/>
        <Var name="param0.Value" value="98" failure="true"/>
      </Input>
    </TestCase>
  </Function>
</TestCases>
